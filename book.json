{
    "title": "GOST DOCS",
    "plugins" : [ 
        "multilingual-index",
        "code",
        "-lunr", 
        "-search", 
        "search-pro",
        "back-to-top-button",
		"page-toc"
    ],
    "pluginsConfig": { 
        "multilingual-index": {
            "enableSuper": false,
            "index":"index.html"
        },
		"page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first",
            "showByDefault": true
        }
	}
}
